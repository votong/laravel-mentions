{
    "name": "votong/laravel-mentions",
    "description": "End-to-end mentions in Laravel 5, 6, 7 & 8.",
    "keywords": [
        "laravel",
        "mentions"
    ],
    "homepage": "https://github.com/votong/laravel-mentions",
    "license": "MIT",
    "authors": [
        {
            "name": "Vo Tong",
            "email": "votong175@gmail.com",
            "homepage": "https://github.com/votong",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/http": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/routing": "^5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "6.3|^7.0|^8.0|^9.0",
        "orchestra/testbench": "3.5"
    },
    "autoload": {
        "psr-4": {
            "VoTong\\Mentions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VoTong\\Mentions\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor\\bin\\phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "VoTong\\Mentions\\MentionServiceProvider"
            ]
        }
    }
}
